   "source": [
    "from sympy import primerange\n",
    "\n",
    "primes = set(primerange(1000000))\n",
    "count = len(primes)\n",
    "for prime in primes:\n",
    "    s = str(prime)\n",
    "    n = len(s)\n",
    "    s *= 2\n",
    "    i = 0\n",
    "    while i < n:\n",
    "        if int(s[i:i+n]) not in primes:\n",
    "            count -= 1\n",
    "            break\n",
    "        i += 1\n",
    "\n",
    "count"
   ]
